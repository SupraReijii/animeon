{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": [
        "arn:aws:s3:::video/*/1080/*",
        "arn:aws:s3:::video/*/720/*",
        "arn:aws:s3:::video/*/480/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": "s3:PutObject",
      "Resource": [
        "arn:aws:s3:::video",
        "arn:aws:s3:::video/*"
      ]
    }
  ]
}
